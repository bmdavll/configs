startup_message off

# alt screen for text editors, pagers, etc.
altscreen on

# enable 256 colors
term screen-256color
terminfo rxvt-unicode 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'

# use X scrolling
termcapinfo xterm* ti@:te@

# start at screen 1
bind c  screen 1
bind ^c screen 1
bind 0  select 10
screen 1
